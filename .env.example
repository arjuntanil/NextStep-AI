# NextStepAI Environment Variables
# Copy this file to .env and fill in your actual values

# Google API Key for Gemini LLM
GOOGLE_API_KEY=your_google_api_key_here

# Google OAuth Credentials (for user authentication)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# JWT Secret Key (generate a random secure key)
JWT_SECRET_KEY=your_jwt_secret_key_here

# Frontend URL
STREAMLIT_FRONTEND_URL=http://localhost:8501

# Optional: Adzuna API (for live job postings)
# Sign up at https://developer.adzuna.com/
# ADZUNA_APP_ID=your_adzuna_app_id
# ADZUNA_APP_KEY=your_adzuna_app_key
